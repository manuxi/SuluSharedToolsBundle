{
    "name": "manuxi/sulu-shared-tools",
    "description": "Tools for Sulu bundles.",
    "version": "0.0.1",
    "type": "library",
    "homepage": "https://github.com/manuxi/SuluSharedTools",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/doctrine-bundle": "^2.13",
        "phpcr/phpcr-migrations-bundle": "^1.6",
        "sulu/sulu": "^2.5"
    },
    "require-dev": {
        "jackalope/jackalope-doctrine-dbal": "^1.3.4",
        "phpunit/phpunit": "^8.0",
        "symfony/browser-kit": "^4.4 | ^5.0 | ^6.2 | ^7.0",
        "symfony/phpunit-bridge": "^4.4 | ^5.0 | ^6.2 | ^7.0",
        "symfony/console": "^4.4 | ^5.0 | ^6.2 | ^7.0",
        "phpspec/prophecy": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "Manuxi\\SuluSharedTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manuxi\\SuluSharedTools\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "@phpunit"
        ],
        "phpunit": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true
        }
    }
}
